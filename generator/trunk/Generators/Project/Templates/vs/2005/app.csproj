<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>{<%= AppGuid %>}</ProjectGuid>
    <RootNamespace><%= ClassName %></RootNamespace>
    <AssemblyName><%= ClassName %></AssemblyName>
    <OutputType>Library</OutputType>
    <NoConfig>false</NoConfig>
    <OutputPath>public\bin\</OutputPath>
    <RegisterForComInterop>False</RegisterForComInterop>
    <GenerateSerializationAssemblies>Auto</GenerateSerializationAssemblies>
    <BaseAddress>4194304</BaseAddress>
    <PlatformTarget>AnyCPU</PlatformTarget>
    <FileAlignment>4096</FileAlignment>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <NoStdLib>false</NoStdLib>
    <WarningLevel>4</WarningLevel>
    <Optimize>false</Optimize>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <CheckForOverflowUnderflow>false</CheckForOverflowUnderflow>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <NoStdLib>false</NoStdLib>
    <WarningLevel>4</WarningLevel>
    <Optimize>true</Optimize>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <CheckForOverflowUnderflow>false</CheckForOverflowUnderflow>
    <DefineConstants>TRACE</DefineConstants>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DebugType>Full</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DebugType>None</DebugType>
    <DebugSymbols>false</DebugSymbols>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="NVelocity">
      <HintPath>..\..\lib\Castle\net-2.0\NVelocity.dll</HintPath>
    </Reference>
    <Reference Include="Castle.ActiveRecord">
      <HintPath>..\..\lib\Castle\net-2.0\Castle.ActiveRecord.dll</HintPath>
    </Reference>
    <Reference Include="Castle.MonoRail.ActiveRecordScaffold">
      <HintPath>..\..\lib\Castle\net-2.0\Castle.MonoRail.ActiveRecordScaffold.dll</HintPath>
    </Reference>
    <Reference Include="Castle.MonoRail.ActiveRecordSupport">
      <HintPath>..\..\lib\Castle\net-2.0\Castle.MonoRail.ActiveRecordSupport.dll</HintPath>
    </Reference>
    <Reference Include="Castle.MonoRail.Framework">
      <HintPath>..\..\lib\Castle\net-2.0\Castle.MonoRail.Framework.dll</HintPath>
    </Reference>
    <Reference Include="Castle.MonoRail.Framework.Views.NVelocity">
      <HintPath>..\..\lib\Castle\net-2.0\Castle.MonoRail.Framework.Views.NVelocity.dll</HintPath>
    </Reference>
    <Reference Include="System.Web" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Configuration" />
    <Reference Include="NHibernate">
      <HintPath>..\..\lib\Castle\net-2.0\NHibernate.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="Castle.Components.Common.TemplateEngine.NVelocityTemplateEngine">
      <HintPath>..\..\lib\Castle\net-2.0\Castle.Components.Common.TemplateEngine.NVelocityTemplateEngine.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="Castle.Components.Binder">
      <HintPath>..\..\lib\Castle\net-2.0\Castle.Components.Binder.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="Castle.Core">
      <HintPath>..\..\lib\Castle\net-2.0\Castle.Core.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    
  </ItemGroup>
  <Import Project="$(MSBuildBinPath)\Microsoft.CSharp.Targets" />
</Project>